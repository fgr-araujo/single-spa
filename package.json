{
  "name": "single-spa-portal-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-all": "npm run install:config && npm run install:navbar && npm run install:people && npm run install:planets && npm run install:fetcher",
    "install:config": "npm i",
    "install:navbar": "cd ./navbar/ && npm i",
    "install:people": "cd ./people/ && npm i",
    "install:planets": "cd ./planets/ && npm i",
    "install:fetcher": "cd ./fetcher/ && npm i",
    "start": "concurrently --kill-others --kill-others-on-fail -p name --names \"config,common-deps,navbar,people,planets,fetcher\" \"npm run start:config\" \"npm run start:common-deps\" \"npm run start:navbar\" \"npm run start:people\" \"npm run start:planets\" \"npm run start:fetcher\"",
    "start:config": "webpack-dev-server --config ./webpack.config.config.dev.js --port 8233",
    "start:common-deps": "webpack-dev-server --config ./webpack.common-deps.config.dev.js --port 8234",
    "start:navbar": "cd ./navbar/ && webpack-dev-server --config ./webpack.dev.js --port 8235",
    "start:people": "cd ./people/ && webpack-dev-server --config ./webpack.dev.js --port 8236",
    "start:planets": "cd ./planets/ && webpack-dev-server --config ./webpack.dev.js --port 8237",
    "start:fetcher": "cd ./fetcher/ && webpack-dev-server --config ./webpack.dev.js --port 8238",
    "build": "npm run build:config && npm run build:common-deps && npm run build:navbar && npm run build:fetcher",
    "build:config": "webpack --config ./webpack.config.config.js -p",
    "build:common-deps": "webpack --config ./webpack.common-deps.config.js -p",
    "build:navbar": "cd ./navbar/ && webpack --config ./webpack.config.js -p",
    "build:people": "cd ./people/ && webpack --config ./webpack.config.js -p",
    "build:planets": "cd ./planets/ && webpack --config ./webpack.config.js -p",
    "build:fetcher": "cd ./fetcher/ && webpack --config ./webpack.config.js -p",
    "postinstall": "node postinstall.js"
  },
  "author": "Justin McMurdie",
  "license": "ISC",
  "dependencies": {
    "lodash": "4.17.10",
    "rxjs": "6.2.2",
    "single-spa": "4.0.1",
    "sofe": "2.1.4",
    "systemjs": "0.20.19"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.0",
    "clean-webpack-plugin": "0.1.19",
    "concurrently": "4.0.1",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "style-loader": "0.23.0",
    "webpack": "4.17.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7"
  }
}
